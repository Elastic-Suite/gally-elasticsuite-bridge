{
    "name": "gally/elasticsuite-bridge",
    "type": "magento2-component",
    "license": "OSL-3.0",
    "description": "Bridge between Gally and the legacy Elasticsuite",
    "keywords":[ "composer-installer", "magento", "gally", "elasticsuite"],
    "authors": [
        {
            "name": "The Elasticsuite team",
            "email": "elasticsuite@smile.fr"
        }
    ],
    "require": {
        "magento/framework": "^100.1|^101.0|^102.0|^103.0",
        "magento/magento-composer-installer": "*",
        "smile/elasticsuite": ">=2.10.0",
        "gally/gally-php-rest-sdk": "*"
    },
    "require-dev":{
        "composer/composer":"*@dev"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Gally\\ElasticsuiteBridge\\": ""
        }
    }
}
